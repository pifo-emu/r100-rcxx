 #!/bin/bash 

#

#CACHEEX SERVER
P0t3=$(shuf -i 20000-55000 -n 1)
echo $P0t3 > /tmp/port2
echo $P0t3 > /tmp/fir-port

	ip2=$(cat /tmp/fir-port)
	expr $(tail -1 /tmp/port2) + 5 >> /tmp/port2 
	suma=$(tail -1 /tmp/port2)
	echo CONFIGURING CACHEEX SERVER
	echo PORT $suma IS ASSIGNED TO CACHEEX SERVER
 

echo "
#==========================================================
#==                    CACHEEX SERVER                    ==
#==========================================================
CCCAM PORT: $suma
CCCAM VERSION: 2.0.11
CCCAM NODEID: bab3fadd2d36f988
CACHEEX DCW CHECK: YES
CACHEEX KEEPALIVE: NO
CACHEEX CCW: YES
CACHEEX BADCW: NO
CACHEEX SEND BAD CW: NO
CACHEEX STEALTHMODE: NO
 
#F: local cacheex3 { cacheex_mode=3; } # mode reverse desde oscam
 
#C: host.dns puerto user pass { cacheex_mode=2; }
#F: local cacheex3 { cacheex_mode=2; } 
" > /usr/local/etc/multics/INCLUDE/Cacheex.cfg
	
	ip2=$(cat /tmp/fir-port)
	expr $(tail -1 /tmp/port2) + 5 >> /tmp/port2 
	suma=$(tail -1 /tmp/port2)
	echo CONFIGURING CACHEEX SERVER
	echo PORT $suma IS ASSIGNED TO CS378X SERVER

echo "
 
#==========================================================
#==                    CS378X SERVER                     ==
#==========================================================
CS378X PORT : $suma
CS378X KEEPALIVE: YES
CS378X DCW CHECK: YES
CS378X CCW: YES
CS378X BADCW: NO
CS378X SEND BAD CW: NO
CS378X STEALTHMODE: NO
 
 
#CS378X SERVER: hostname puerto user pass { cacheex_mode=2; }
#CS378X USER: user pass { cacheex_mode=2; }
 " >> /usr/local/etc/multics/INCLUDE/Cacheex.cfg
	
	ip2=$(cat /tmp/fir-port)
	expr $(tail -1 /tmp/port2) + 5 >> /tmp/port2 
	suma=$(tail -1 /tmp/port2)
	echo CONFIGURING CACHEEX SERVER
	echo PORT $suma IS ASSIGNED TO CAMD35 SERVER


echo "
#==========================================================
#==                    CAMD35 SERVER                     ==
#==========================================================
CAMD35 PORT: $suma
CAMD35 KEEPALIVE:YES
CAMD35 DCW CHECK:YES
CAMD35 CCW:YES
CAMD35 BADCW:NO
CAMD35 SEND BAD CW:NO
CAMD35 STEALTHMODE: NO
 
#L: host.dns.net 28892 user passs { cacheex_mode=2; }
#CAMD35 USER: user pass { cacheex_mode=2; }

" >> /usr/local/etc/multics/INCLUDE/Cacheex.cfg



#CLUSTERED CACHE
P0t1=$(shuf -i 5000-9999 -n 1)

echo CONFIGURING CLUSTERED CACHE
	echo PORT $suma IS ASSIGNED TO CLUSTERED CACHE
 

echo "
#==========================================================
#== CLUSTERED CACHE (Compatible with CSP cache)          ==
#==========================================================
CACHE PORT: $P0t1
CACHE ALIVETIME: 25
CACHE FILTER: NO
CACHE FILTER TIME: 0
CACHE THRESHOLD: 0 
CACHE AUTOADD: YES,YES
CACHE AUTOADD_NO_ENABLE: NO
CACHE KEEPALIVE: NO
CACHE DCWCHECK2: NO
CACHE DCWCHECK3: NO
#==========================================================
#==              MULTICS.ES,TELEGRAM                     ==
#==========================================================

#CACHE PEER: HOST-OSCAM PUERTO-CSP { csp=yes; nominhits=1 } #para subir cache peer desde oscam

#CACHE PEER: host.dns.net $P0t1 #intecmabio de cache entre servidores multics
" > /usr/local/etc/multics/INCLUDE/Cache.cfg

sleep 5 &&  clear


#PROFILES

P0t3=$(shuf -i 20000-55000 -n 1)
echo $P0t3 > /tmp/port2
echo $P0t3 > /tmp/fir-port

echo CONFIGURING PROFILES

echo "#==========================================================
#              PROFILES DEFAULT VALUES                   ==
#==========================================================
DEFAULT KEY: 01 02 03 04 05 06 07 08 09 10 11 12 13 14
DEFAULT DCW MAXFAILED: 0
DEFAULT SERVER FIRST: 0
DEFAULT SERVER MAX: 0
DEFAULT SERVER THRESHOLD: 0
DEFAULT DCW TIMEOUT: 5500
DEFAULT CACHE TIMEOUT: 175
DEFAULT SERVER INTERVAL: 500
DEFAULT SERVER TIMEOUT: 1000
DEFAULT CACHE VALIDECMTIME: 0
DEFAULT CACHEEX VALIDECMTIME: 0
DEFAULT SERVER VALIDECMTIME: 7000
DEFAULT DCW RETRY: 0
DEFAULT RETRY NEWCAMD: 0
DEFAULT RETRY CCCAM: 0
DEFAULT DCW CHECK: NO
DEFAULT ECM CHECK LENGTH: NO
DEFAULT CACHE SENDREQ: YES
DEFAULT CACHE RESENDREQ: YES
DEFAULT CACHE SENDREP: YES
DEFAULT CACHEEX MAXHOP: 2
DEFAULT ENABLE CCCAM: YES
DEFAULT ENABLE MGCAMD: YES
DEFAULT ENABLE NEWCAMD: YES
DEFAULT ENABLE CACHE: YES
DEFAULT ENABLE CACHEEX: YES
DEFAULT ACCEPT NULL SID: NO
DEFAULT ACCEPT NULL CAID: YES
DEFAULT ACCEPT NULL PROVIDER: YES
DEFAULT CACHE STATIC: YES
UPDATE ONCHANGE: YES
LOGLEVEL: 4 # DEBUG

#==========================================================
#                     PROFILES SCS                       ==
#==========================================================

" > /usr/local/etc/multics/INCLUDE/Profiles.cfg


echo -e "\e[1;40;35mDO YOU WANT TO CONFIGURE? [MOVISTAR] 1810 ?\e[0m" 

   echo -e "\e[1;40;37m1) \e[1;40;32mSI\e[0m"
   echo -e "\e[1;40;37m2) \e[1;40;31mNO\e[0m"

read red0
   case $red0 in

       1) echo CONFIGURING [MOVISTAR]
	echo THE PORT $P0t3 IS ASSIGNED 
echo "
[MOVISTAR]
PORT: $P0t3
CAID: 1810
PROVIDERS: 0
ECM CHECK LENGTH: NO
ECM LENGTH: 8E
DCW CHECK: YES
DCW CYCLE_CHECK: YES
SERVER THRESHOLD: 0
SERVER CACHE_THRESHOLD: 0
DCW ECM_HEADER_ACTIVE: NO
DCW ECM_HEADER_VALUES: 80308B078400,81308B078400

" >> /usr/local/etc/multics/INCLUDE/Profiles.cfg

           sleep 1 &&  clear ;;

       2) echo SEGUIMOS 

           sleep 1 &&  clear ;;
       
esac

echo -e "\e[1;40;35mDO YOU WANT TO CONFIGURE? [ALL SEX 500] ?\e[0m" 

