#CCCAM SERVER
P0t3=$(shuf -i 20000-55000 -n 1)

	echo CONFIGURING CCCAM SERVER
	echo SE A ASIGNADO EL PUERTO $P0t3 A CCCAM SERVER
 

echo "
#==========================================================
#==                    CCCAM VISIONADO                   ==
#==========================================================
CCCAM VERSION: 2.0.11
CCCAM NODEID: $(shuf -i0000000000000000-9999999999999999 -n1)
CCCAM KEEPALIVE: NO
CCCAM PORT: $P0t3
CCCAM STEALTHMODE: NO
CCCAM DCW CHECK: YES   
CCCAM PROFILES: $(cat /tmp/fir-port)
 
#F: user pass # asi damos de todos los perfiles del default de arriba

#F: user pass {aqui dentro podemos poner los perfiles que queremos dar separados por comas}
" > /usr/local/etc/multics/INCLUDE/CCcam.cfg

#MGCAMD SERVER
P0t3=$(shuf -i 20000-55000 -n 1)

	echo CONFIGURING MGCAMD SERVER
	echo SE A ASIGNADO EL PUERTO $P0t3 A MGCAMD SERVER
 

echo "
#==========================================================
#==              MGCAMD GLOBAL USERS                     ==
#==========================================================
MGCAMD PORT: $P0t3
MGCAMD KEEPALIVE: NO
MGCAMD DCW CHECK: NO
MGCAMD SEND BAD CW:NO
MGCAMD FILTER: ON
MGCAMD KEY: 01 02 03 04 05 06 07 08 09 10 11 12 13 14
MGCAMD PROFILES: $(cat /tmp/fir-port)

#==========================================================
#==              MGCAMD SERVERS                          ==
#==========================================================

